{
    "name": "fileeye/mediaprobe",
    "type": "library",
    "description": "A PHP library for reading and writing media files metadata.",
    "keywords": [
        "image", "exif", "media"
    ],
    "homepage": "https://github.com/FileEye/MediaProbe",
    "license": "GPL-3.0",
    "require": {
        "php": ">=7.4",
        "ext-gd": "*",
        "ext-mbstring": "*",
        "composer-runtime-api": "^2.0.0",
        "psr/log": "*",
        "monolog/monolog": "^3",
        "shanethehat/pretty-xml": "*",
        "sibche/plist": "*",
        "symfony/stopwatch": "^6"
    },
    "require-dev" : {
        "ext-exif": "*",
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "*",
        "symfony/console": "^6",
        "symfony/finder": "^6",
        "symfony/filesystem": "^6",
        "symfony/process": "^6",
        "symfony/var-dumper": "^6",
        "symfony/yaml": "^6",
        "bramus/monolog-colored-line-formatter": "^3"
    },
    "autoload": {
        "psr-4": {
            "FileEye\\MediaProbe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FileEye\\MediaProbe\\Test\\": "tests/"
        }
    },
    "bin": ["bin/fileeye-mediaprobe"]
}
