{
    "name": "fileeye/imageinfo",
    "type": "library",
    "description": "A PHP library for reading and writing Exif headers in JPEG and TIFF images.",
    "keywords": [
        "image", "exif"
    ],
    "homepage": "https://github.com/FileEye/ImageInfo",
    "license": "GPL-3.0",
    "require": {
        "php": ">=5.4.0",
        "ext-gd": "*",
        "ext-gmp": "*",
        "psr/log": "*",
        "monolog/monolog": "*",
        "rodneyrehm/plist": "*"
    },
    "require-dev" : {
        "ext-exif": "*",
        "phpunit/phpunit": "<9",
        "squizlabs/php_codesniffer": "*",
        "symfony/console": "*",
        "symfony/finder": "*",
        "symfony/filesystem": "*",
        "symfony/var-dumper": "*",
        "symfony/yaml": "*",
        "bramus/monolog-colored-line-formatter": "^2.0",
        "shanethehat/pretty-xml": "*"
    },
    "autoload": {
        "psr-4": {
            "FileEye\\ImageInfo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FileEye\\ImageInfo\\Test\\core\\": "test/"
        }
    },
    "bin": ["bin/fileeye-imageinfo"]
}
