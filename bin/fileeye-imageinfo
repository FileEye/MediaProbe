#!/usr/bin/env php
<?php
/**
 * A Symfony CLI utility application for ImageInfo.
 */

use FileEye\ImageInfo\core\ImageInfo;
use FileEye\ImageInfo\core\Command\CompileCommand;
use FileEye\ImageInfo\core\Command\DumpCommand;
use Symfony\Component\Console\Application;

require_once __DIR__ . '/../vendor/autoload.php';

$application = new Application('fileeye-imageinfo', ImageInfo::version());
$application->add(new CompileCommand());
$application->add(new DumpCommand());
$application->run();
